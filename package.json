{
  "name": "atp-cli",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI for Agent Trust Protocol v1.0 â€” identity, attestations, receipts, inscriptions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "atp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "atp": "node dist/index.js",
    "test": "vitest run --exclude test/regtest.test.ts",
    "test:regtest": "vitest run test/regtest.test.ts",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "atp",
    "agent",
    "trust",
    "protocol",
    "bitcoin",
    "ed25519",
    "inscription"
  ],
  "author": "Shrike (@ShrikeBot)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShrikeBot/atp-cli"
  },
  "dependencies": {
    "@noble/curves": "^1.8.0",
    "cbor-x": "^1.6.0",
    "commander": "^14.0.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  }
}
